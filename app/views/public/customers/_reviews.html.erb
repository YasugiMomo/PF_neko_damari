
  <P>あなたのレビュー</P>
  <table>
    <tr>
      <th>猫カフェ</th>
      <th>評価</th>
      <th>タイトル</th>
      <th></th>
    </tr>
    <% reviews.each do |review| %>
    <tr>
      <td><%= link_to review.shop.name, shop_path(review.shop_id) %></td>
      <td><%= review.rate %></td>
      <td><%= link_to review.title, shop_review_path(review.shop_id, review.id) %></td>
      <td><p><%= link_to "#{review.comments.count} コメント", shop_review_path(review.shop_id, review.id) %></p></td>
    </tr>
    <% end %>
  </table>
